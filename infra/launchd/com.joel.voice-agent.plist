<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.joel.voice-agent</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/joel/Code/joelhooks/joelclaw/infra/voice-agent/start.sh</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/joel/Projects/joelclaw-voice-agent</string>

